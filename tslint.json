{
  "extends": [
    "tslint:all"
  ],
  "rules": {
    "max-line-length": [ true, 200 ],
    "semicolon": [ false, "always" ],
    "quotemark": [ true, "single" ],
    "trailing-comma": [ true, { "multiline": "never", "singleline": "never" } ],
    "no-trailing-whitespace": false,
    "array-type": [
      true,
      "array"
    ],
    "arrow-return-shorthand": false,
    "ban-comma-operator": true,
    "binary-expression-operand-order": false,
    "comment-format": false,
    "completed-docs": false,
    "cyclomatic-complexity": false,
    "deprecation": false,
    "encoding": true,
    "eofline": false,
    "file-name-casing": [
      true,
      "kebab-case"
    ],
    "forin": false,
    "increment-decrement": [
      true,
      "allow-post"
    ],
    "interface-name": false,
    "interface-over-type-literal": false,
    "jsdoc-format": false,
    "match-default-export-name": false,
    "max-classes-per-file": false,
    "max-file-line-count": false,
    "member-access": true,
    "member-ordering": false,
    "missing-jsdoc": false,
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-angle-bracket-type-assertion": false,
    "no-any": false,
    "no-boolean-literal-compare": false,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace",
      "log"
    ],
    "no-default-export": true,
    "no-duplicate-case": false,
    "no-duplicate-imports": true,
    "no-duplicate-switch-case": true,
    "no-empty": false,
    "no-empty-interface": false,
    "no-empty-interfaces": false,
    "no-empty-line-after-opening-brace": false,
    "no-floating-promises": false,
    "no-for-in": false,
    "no-for-in-array": true,
    "no-function-constructor-with-string-args": false,
    "no-implicit-dependencies": false,
    "no-import-side-effect": [
      true,
      "zone.js"
    ],
    "no-inferrable-types": false,
    "no-internal-module": false,
    "no-magic-numbers": false,
    "no-multiline-string": false,
    "no-namespace": false,
    "no-null-keyword": false,
    "no-object-literal-type-assertion": true,
    "no-parameter-properties": false,
    "no-parameter-reassignment": false,
    "no-redundant-jsdoc": true,
    "no-relative-imports": false,
    "no-require-imports": false,
    "no-reserved-keywords": false,
    "no-return-await": true,
    "no-shadowed-variable": false,
    "no-stateless-class": false,
    "no-submodule-imports": false,
    "no-this-assignment": true,
    "no-unbound-method": false,
    "no-unexternalized-strings": false,
    "no-unnecessary-bind": false,
    "no-unnecessary-callback-wrapper": false,
    "no-unnecessary-class": false,
    "no-unnecessary-qualifier": false,
    "no-unnecessary-type-assertion": false,
    "no-unsafe-any": false,
    "no-unused-variable": false,
    "no-use-before-declare": false,
    "no-var-self": false,
    "no-void-expression": {
      "options": "ignore-arrow-function-shorthand"
    },
    "object-literal-shorthand": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": [
      true,
      "allow-declarations",
      "allow-named-functions"
    ],
    "ordered-imports": false,
    "prefer-conditional-expression": false,
    "prefer-for-of": false,
    "prefer-function-over-method": false,
    "prefer-object-spread": true,
    "prefer-readonly": false,
    "prefer-switch": false,
    "prefer-template": false,
    "react-tsx-curly-spacing": false,
    "restrict-plus-operands": false,
    "strict-boolean-expressions": [
      true,
      "ignore-rhs"
    ],
    "strict-comparisons": [
      true,
      {
        "allow-object-equal-comparison": true,
        "allow-string-order-comparison": true
      }
    ],
    "strict-type-predicates": false,
    "switch-final-break": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "type-literal-delimiter": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration",
      "array-destructuring"
    ],
    "typedef-whitespace": false,
    "use-default-type-parameter": false,
    "valid-typeof": false,
    "invalid-void": false
  }
}
